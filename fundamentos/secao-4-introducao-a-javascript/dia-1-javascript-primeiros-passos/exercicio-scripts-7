// Utilize if/else para fazer um programa que converta uma nota dada em porcentagem (de 0 a 100) em conceitos de A a F. Siga essas regras:
// Porcentagem >= 90 -> A
// Porcentagem >= 80 -> B
// Porcentagem >= 70 -> C
// Porcentagem >= 60 -> D
// Porcentagem >= 50 -> E
// Porcentagem < 50 -> F
// O programa deve retornar uma mensagem de erro e encerrar se a nota passada for menor que 0 ou maior que 100.

let score = 95
if (score < 0 || score > 100) {
    console.log("A porcentagem " + score + " é inválida! Tente novamente.")
}
else if (score >= 90) {
    console.log ("Uau! Que demais! Sua nota é A!")
}
else if (score >= 80 && score < 90) {
    console.log ("Parabéns! Sua nota é B!")
}
else if (score >= 70 && score < 80) {
    console.log ("Ok! Sua nota é C!")
}
else if (score >= 60 && score < 70) {
    console.log ("Vamos estudar mais? Sua nota é D.")
}
else if (score >= 50 && score < 60) {
    console.log ("Sua nota é E. Busque ajuda enquanto há tempo!")
}
else {
    console.log ("Sua nota é F e não atingiu a média. Busque ajuda e tente novamente.")
}
